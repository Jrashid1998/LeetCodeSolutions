//https://leetcode.com/problems/jewels-and-stones/

class Solution {
    public int numJewelsInStones(String J, String S) {

        int counter = 0;
        
        for(int i=0; i < J.length();i++){
            
            for(int x=0;x<S.length();x++){
                
                if(J.charAt(i) == S.charAt(x)){
                    counter++;
                    }
                }
            }
        return counter;
                
        }
    }
